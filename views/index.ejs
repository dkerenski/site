                pathPrepare($svg_14);
                pathPrepare($svg_15);
                pathPrepare($svg_16);
                pathPrepare($svg_17);
                pathPrepare($svg_18);
                pathPrepare($svg_19);
                pathPrepare($svg_20);


                // init controller
                var controller = new ScrollMagic();

                // build tween
                var tween = new TimelineMax()
                    // .add(TweenMax.to($svg_6, 0.9, {strokeDashoffset: 0, ease:Linear.easeNone})) // draw word for 0.9
                    .add(TweenMax.to($svg_7, 0.1, {strokeDashoffset: 0, ease:Linear.easeNone}))
