    <script>
        $(document).ready(function populateCounters() {
            //Set the two dates
            var millennium = new Date(2013, 5, 1) //Month is 0-11 in JavaScript
            today = new Date()
            //Get 1 day in milliseconds
            var one_day=1000*60*60*24

            //Calculate difference btw the two dates, and convert to days
            var dateDifference = Math.ceil((today.getTime()-millennium.getTime())/(one_day));

            // var base = dateDifference;
            var weeks = Math.round(dateDifference / 7);

            $('.git-counter').html(dateDifference * 123);
            $('.coffee-counter').html(dateDifference * 8);
            $('.pizza-counter').html(weeks * 3);
            $('.code-counter').html(dateDifference * 764);

            $('.git-counter').counterUp({
                delay: 10,
                time: 1000
            });

            $('.coffee-counter').counterUp({
                delay: 10,
                time: 1000
            });

             $('.pizza-counter').counterUp({
                delay: 20,
                time: 1000
            });

             $('.code-counter').counterUp({
                delay: 10,
                time: 1000
            });
        });


        var theForm = document.getElementById( 'theForm' );

        new stepsForm( theForm, {
            onSubmit : function( form ) {
                // hide form
                classie.addClass( theForm.querySelector( '.simform-inner' ), 'hide' );

                /*
                form.submit()
                or
                AJAX request (maybe show loading indicator while we don't have an answer..)
                */

                // let's just simulate something...
                var messageEl = theForm.querySelector( '.final-message' );
                messageEl.innerHTML = 'Thank you! We\'ll be in touch.';
                classie.addClass( messageEl, 'show' );
            }
        } );

         $(document).ready(function($) {

            if ((window.innerWidth > 700))  {
                  function pathPrepare($el) {
                    // console.log($el[0]);
                    var lineLength = $el[0].getTotalLength();
                    $el.css("stroke-dasharray", lineLength);
                    $el.css("stroke-dashoffset", lineLength);
                }

                // var $svg_6 = $("path#svg_6");
                var $svg_7 = $("path#svg_7");
                var $svg_8 = $("path#svg_8");
                var $svg_9 = $("path#svg_9");
                var $svg_10 = $("path#svg_10");
                var $svg_11 = $("path#svg_11");
                var $svg_12 = $("path#svg_12");
                var $svg_13 = $("path#svg_13");
                var $svg_14 = $("path#svg_14");
                var $svg_15 = $("path#svg_15");
                var $svg_16 = $("path#svg_16");
                var $svg_17 = $("path#svg_17");
                var $svg_18 = $("path#svg_18");
                var $svg_19 = $("path#svg_19");
                var $svg_20 = $("path#svg_20");


                // prepare SVG
                // pathPrepare($svg_6);
                pathPrepare($svg_7);
                pathPrepare($svg_8);
                pathPrepare($svg_9);
                pathPrepare($svg_10);
                pathPrepare($svg_11);
